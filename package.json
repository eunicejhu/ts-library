{
  "name": "@zuoqinhu/utils",
  "version": "1.1.11",
  "author": "zuoqinhu",
  "description": "This is a utility library written in typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest --watch",
    "build:cjs": "npx tsc --project tsconfig.build.json ",
    "build:esm": "npx tsc --project tsconfig.build.json -m es2015 --outDir dist-esm ",
    "clean": "rm -rf dist/ dist-esm/",
    "publish": "npm publish --access public",
    "quickpublish": "npm run build:cjs && npm run publish"
  },
  "keywords": [
    "typescript",
    "library"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/eunicejhu/ts-library"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.5.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@types/lodash.isequal": "^4.5.5",
    "csstype": "^3.0.11",
    "lodash.isequal": "^4.5.0"
  }
}
